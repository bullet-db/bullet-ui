{
  "name": "bullet-ui",
  "version": "1.0.0",
  "description": "Ember 3 UI for Bullet - Realtime Query Engine",
  "private": true,
  "main": "express-server.js",
  "keywords": [
    "Big Data",
    "Streaming",
    "Query Engine",
    "Look Forward",
    "No Persistence",
    "Ember 3"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "clean": "rm -rf tmp dist node_modules",
    "build": "ember build --environment=production",
    "test": "COVERAGE=true ember test",
    "local-build": "ember build",
    "local-test": "ember test --server",
    "start": "ember server",
    "lint": "yarn run eslint config/* app/* tests/*",
    "lint-fix": "yarn run eslint config/* app/* tests/* --fix",
    "build-package": "yarn && yarn run -s build &&  rm -rf node_modules && yarn install --production && yarn run -s archive",
    "archive": "tar --exclude='.eslintrc.js' --exclude='.DS_Store' --exclude='coverage.js' --exclude='environment.js' -czf bullet-ui-v${BULLET_VERSION}.tar.gz dist express-server.js server config node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bullet-db/bullet-ui"
  },
  "bugs": {
    "url": "https://github.com/bullet-db/bullet-ui/issues"
  },
  "engines": {
    "node": ">= 10"
  },
  "author": "Akshai Sarma akshaisarma@gmail.com",
  "contributors": [
    {
      "name": "Akshai Sarma",
      "email": "akshaisarma@gmail.com",
      "url": "http://github.com/akshaisarma"
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@ember/jquery": "^1.1.0",
    "@ember/optional-features": "^1.3.0",
    "@stomp/stompjs": "~4.0.1",
    "broccoli-asset-rev": "~2.4.5",
    "browserify-zlib": "~0.2.0",
    "c3": "~0.5.3",
    "chart.js": "~2.7.2",
    "ember-ajax": "~4.0.1",
    "ember-browserify": "~1.2.2",
    "ember-cli": "~3.16.0",
    "ember-cli-app-version": "~3.2.0",
    "ember-cli-babel": "~7.19.0",
    "ember-cli-bootstrap-sassy": "0.5.8",
    "ember-cli-chart": "~3.6.0",
    "ember-cli-clipboard": "~0.14.0",
    "ember-cli-code-coverage": "1.0.0-beta.9",
    "ember-cli-dependency-checker": "~3.2.0",
    "ember-cli-htmlbars": "~4.3.1",
    "ember-cli-inject-live-reload": "~2.0.2",
    "ember-cli-pretender": "~3.2.0",
    "ember-cli-sass": "~10.0.0",
    "ember-cli-uglify": "~3.0.0",
    "ember-cp-validations": "4.0.0-beta.10",
    "ember-data": "~3.16.0",
    "ember-font-awesome": "4.0.0-rc.4",
    "ember-light-table": "2.0.0-beta.4",
    "ember-load-initializers": "~2.1.1",
    "ember-localforage": "1.5.8",
    "ember-moment": "8.0.0",
    "ember-power-select": "~4.0.0",
    "ember-progress-bar": "^1.0.0",
    "ember-promise-helpers": "~1.0.3",
    "ember-qunit": "~4.6.0",
    "ember-radio-button": "2.0.0",
    "ember-resolver": "~8.0.0",
    "ember-responsive": "~3.0.5",
    "ember-route-action-helper": "2.0.8",
    "ember-sinon": "~5.0.0",
    "ember-source": "~3.16.0",
    "ember-tether": "2.0.0",
    "ember-tooltips": "~3.4.2",
    "eslint": "~6.8.0",
    "eslint-plugin-ember": "~8.4.0",
    "eslint-plugin-html": "~6.0.2",
    "eslint-plugin-json": "~2.1.1",
    "eslint-plugin-node": "~11.1.0",
    "file-saver": "~2.0.2",
    "interactjs": "~1.9.10",
    "jQuery-QueryBuilder-Placeholders": "bullet-db/jQuery-QueryBuilder-Placeholders#v1.0.1",
    "jQuery-QueryBuilder-Subfield": "bullet-db/jQuery-QueryBuilder-Subfield#v1.0.2",
    "jquery-ui": "~1.12.1",
