 <div class="labeled-input" ...attributes>
  <label for="{{this.id}}">{{@fieldName}}</label>
  <Input @type={{@type}} @disabled={{@disabled}} @value={{@fieldValue}} placeholder={{@placeholder}} @maxlength={{@maxlength}} @id={{this.id}} class="form-control" />
</div>
