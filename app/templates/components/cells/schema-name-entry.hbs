<span class="schema-name">
  {{extractedValue}}
</span>
{{#if row.hasEnumerations}}
  <span class="schema-enumeration-caret">
    {{#if row.expanded}}
      {{fa-icon "chevron-down" class="expanded-caret"}}
    {{else}}
      {{fa-icon "chevron-right" class="expand-caret"}}
    {{/if}}
  </span>
{{/if}}
